{
  "coverageReporters": [
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 90,
      "branches": 90,
      "functions": 85,
      "lines": 90
    }
  },
  "setupFiles": [
    "<rootDir>/test/shim.js",
    "<rootDir>/test/setup.js"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "<rootDir>/test/mock.js"
  },
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ]
}
