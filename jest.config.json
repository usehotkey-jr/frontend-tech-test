{
  "coverageReporters": [
    "text-summary"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 90,
      "branches": 90,
      "functions": 85,
      "lines": 90
    }
  },
  "setupFiles": [
    "<rootDir>/test/shim.js",
    "<rootDir>/test/jestSetup.js"
  ],
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ]
}
